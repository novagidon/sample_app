<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "приложуха", root_path, id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Домашняя страница", root_path %></li>
        <li><%= link_to "Помощь", help_path %></li>
        <% if logged_in? %>
          <li><%= link_to "Юзеры", users_path %></li>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Аккаунт <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "Профилёк", current_user %></li>
              <li><%= link_to "Настройки", edit_user_path(current_user) %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "логаутнуться", logout_path, method: :delete %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Логинуться", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>